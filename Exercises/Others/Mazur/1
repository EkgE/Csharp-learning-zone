using System;
using System.Linq;
namespace Test
{
    internal class Program
    {
        private static void Main(string[] args)
        {
            Random rnd = new Random();
            int[] array = Enumerable.Repeat(1, rnd.Next(5, 10000)).ToArray();
            Console.WriteLine("Your array contain {0} ones!", arrLength(array));
            Console.ReadKey();
        }
        public static int arrLength(int[] array)
        {
            return array.Length;
        }
    }

}
